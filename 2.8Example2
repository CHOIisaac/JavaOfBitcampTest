package BitTest;

// 1단 ~ 9단을 입력 받아 입력 받은 단수를 출력하는 프로그램. 입력 값이 0이하거나 10이상인 경우 1~9까지의 입력할 것을 출력할 것.
public class Example2 {

	public static void main(String[] args) {
		int input = Integer.parseInt(args[0]);
		
		if(!(0<input)||(input>10)) {
			System.out.println("1~9의 단수를 입력해주세요.");
		}else {
			for(int i = 1; i <=9; i++) {
				System.out.println(input+"*"+i+"="+input*i);
		}
	}
	}
}
